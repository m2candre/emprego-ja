<h1>Criar Empresa</h1>

<%= form_for @company do |f| %>
     <%= f.label :name, "Nome" %>
     <%= f.text_field :name %> <br>
     <%= f.label :location, 'Local' %>
     <%= f.text_field :location %> <br>
     <%= f.label :mail, 'Email' %>
     <%= f.text_field :mail %> <br>
     <%= f.label :phone, 'Telefone' %>
     <%= f.text_field :phone %> <br>
  <%= f.submit "Criar Empresa"%>
<% end %>
